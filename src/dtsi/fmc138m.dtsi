/ {
    model = "fmc138m-v11";

    aliases {
        ethernet0 = &gem0;
        i2c0 = &i2c0;
        serial0 = &uart0;
        serial1 = &uart1;
        spi0 = &qspi;
    };

    chosen {
        /*bootargs = "console=ttyPS0,115200 earlyprintk root=/dev/mmcblk1p2 rootfstype=ext4 nolock rw rootwait cpuidle.off=1 vmalloc=512M";*/
        bootargs = "console=ttyPS0,115200 earlyprintk root=/dev/mmcblk1p2 rootfstype=ext4 nolock rw rootwait cpuidle.off=1 iommu=off";
        /*bootargs = "console=ttyPS0,115200 earlyprintk cpuidle.off=1 iommu=off";*/
        stdout-path = "serial0:115200ns";
    };
};

&sdhci0 {
    status = "okay";
    clock-frequency = <100000000>;
    xlnx,mio_bank = <0x0>;
};

&sdhci1 {
    status = "okay";
    clock-frequency = <100000000>;
    no-1-8-v;
    xlnx,mio_bank = <1>;
    xlnx,has-cd = <0x1>;
    xlnx,has-power = <0x0>;
    xlnx,has-wp = <0x1>;
    disable-wp;
};

&gem0 {
    status = "okay";
    phy-handle = <&phy0>;
    phy-mode = "rgmii-id";
    ps7_ethernet_0_mdio: mdio {
        phy0: phy@0 {
            device_type = "ethernet-phy";
            reg = <0x0>;
            marvell,reg-init =
                /* Fix rx and tx clock transition timing */
                <2 0x15 0xffcf 0>, /* Reg 2,21 Clear bits 4, 5 */
                /* Coopper fifo depth */
                <2 0x10 0 0xd448>; /* Reg 2,16 <- 0xd448 */
            };
    };
};

&sata {
    /* SATA OOB timing settings */
    ceva,p0-cominit-params = /bits/ 8 <0x18 0x40 0x18 0x28>;
    ceva,p0-comwake-params = /bits/ 8 <0x06 0x14 0x08 0x0E>;
    ceva,p0-burst-params = /bits/ 8 <0x13 0x08 0x4A 0x06>;
    ceva,p0-retry-params = /bits/ 16 <0x96A4 0x3FFC>;

    ceva,p1-cominit-params = /bits/ 8 <0x18 0x40 0x18 0x28>;
    ceva,p1-comwake-params = /bits/ 8 <0x06 0x14 0x08 0x0E>;
    ceva,p1-burst-params = /bits/ 8 <0x13 0x08 0x4A 0x06>;
    ceva,p1-retry-params = /bits/ 16 <0x96A4 0x3FFC>;
    phy-names = "sata-phy";
    phys = <&lane3 1 1 3 125000000>;
};

&usb0 {
    status = "okay";
    phy_type = "ulpi";
    dr_mode = "host";
};

&usb1 {
    status = "okay";
    phy_type = "ulpi";
    dr_mode = "host";
};

&dwc3_0 {
    status = "okay";
    dr_mode = "host";
    phy-names = "usb3-phy";
    compatible = "snps,dwc3";
    maximum-speed = "super-speed";
    reg = <0x0 0xfe200000 0x0 0x40000>;
    snps,quirk-frame-length-adjustment = <0x20>;
    snps,refclk_fladj;
    snps,enable_guctl1_resume_quirk;
    snps,enable_guctl1_ipd_quirk;
    snps,xhci-stream-quirk;
    phys = <&lane2 4 0 2 26000000>;
};

&dwc3_1 {
    status = "okay";
    dr_mode = "host";
    phy-names = "usb3-phy";
    /delete-property/ phy-names;
    /delete-property/ phys;
    maximum-speed = "high-speed";
    snps,dis_u2_susphy_quirk;
    snps,dis_u3_susphy_quirk;
};

&zynqmp_dpsub {
    phy-names = "dp-phy0","dp-phy1";
    phys = <&lane1 5 0 0 27000000>, <&lane0 5 1 0 27000000>;
    status = "okay";
    xlnx,max-lanes = <2>;
};

&xdma_0 {
    status = "okay";
};

